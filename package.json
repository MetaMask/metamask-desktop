    "setup": "yarn install && yarn setup:postinstall",
    "setup:postinstall": "yarn patch-package && yarn allow-scripts",
    "start": "yarn build:dev dev --apply-lavamoat=false",
    "build:desktop": "yarn build:desktop:ui && yarn build:desktop:app",
    "build:desktop:app": "./development/build-desktop.sh",
    "build:desktop:app:mv3": "ENABLE_MV3=true yarn build:desktop:app",
    "build:desktop:app:test": "IN_TEST=true SEGMENT_HOST='https://api.segment.io' SEGMENT_WRITE_KEY='FAKE' PHISHING_WARNING_PAGE_URL=http://localhost:9999/ METAMASK_DEBUG=1 yarn build:desktop:app",
    "build:desktop:extension": "yarn start --build-type desktopextension",
    "build:desktop:extension:test": "yarn start:test --build-type desktopextension",
    "build:desktop:extension:mv3": "ENABLE_MV3=true yarn start --build-type desktopextension",
    "build:desktop:ui": "node development/desktop/electron/build/desktop-ui.js dev --apply-lavamoat=false",
    "build:desktop:ui:lavamoat": "node development/desktop/electron/build/desktop-ui.js dev --apply-lavamoat=true",
    "package:desktop:linux": "electron-builder -l",
    "package:desktop:mac": "electron-builder -m",
    "package:desktop:mac:ci": "electron-builder --config electron-builder.ci.json -m",
    "package:desktop:win": "electron-builder -w",
    "package:desktop:win:dev": "set CSC_LINK=desktop_packaging/windows/test_certificate.pfx && set CSC_KEY_PASSWORD=test123* && electron-builder -w",
    "release:desktop:linux": "electron-builder -l -p 'onTagOrDraft'",
    "generate:certificate:win": "@powershell ./desktop_packaging/windows/generate_certificate.ps1",
    "start:desktop": "electron dist_desktop/app/scripts/background.js",
    "start:desktop:test": "GLOBAL_AGENT_HTTP_PROXY=http://127.0.0.1:8000 NODE_TLS_REJECT_UNAUTHORIZED=0 yarn start:desktop",
    "start:lavamoat": "yarn build:dev dev --apply-lavamoat=true",
    "start:mv3": "ENABLE_MV3=true yarn build:dev dev --apply-lavamoat=false",
    "dist": "yarn build dist",
    "dist:desktop:ui": "yarn lavamoat:desktop:ui:build dist",
    "build": "yarn lavamoat:build",
    "build:dev": "node development/build/index.js",
    "start:test": "SEGMENT_HOST='https://api.segment.io' SEGMENT_WRITE_KEY='FAKE' SENTRY_DSN_DEV=https://fake@sentry.io/0000000 PORTFOLIO_URL=http://127.0.0.1:8080 yarn build testDev",
    "benchmark:chrome": "SELENIUM_BROWSER=chrome node test/e2e/benchmark.js",
    "benchmark:desktop:encryption": "node dist_desktop/app/scripts/desktop/benchmarks/encryption.js",
    "benchmark:desktop:web-socket-stream": "node dist_desktop/app/scripts/desktop/benchmarks/web-socket-stream.js",
    "mv3:stats:chrome": "SELENIUM_BROWSER=chrome ENABLE_MV3=true node test/e2e/mv3-perf-stats/index.js",
    "user-actions-benchmark:chrome": "SELENIUM_BROWSER=chrome node test/e2e/user-actions-benchmark.js",
    "benchmark:firefox": "SELENIUM_BROWSER=firefox node test/e2e/benchmark.js",
    "build:test": "SEGMENT_HOST='https://api.segment.io' SEGMENT_WRITE_KEY='FAKE' SENTRY_DSN_DEV=https://fake@sentry.io/0000000 PORTFOLIO_URL=http://127.0.0.1:8080 yarn build test",
    "build:test:flask": "yarn build test --build-type flask",
    "build:test:mv3": "ENABLE_MV3=true SEGMENT_HOST='https://api.segment.io' SEGMENT_WRITE_KEY='FAKE' SENTRY_DSN_DEV=https://fake@sentry.io/0000000 PORTFOLIO_URL=http://127.0.0.1:8080 yarn build test",
    "build:test:desktop": "SKIP_OTP_PAIRING_FLOW=1 SEGMENT_HOST='https://api.segment.io' SEGMENT_WRITE_KEY='FAKE' SENTRY_DSN_DEV=https://fake@sentry.io/0000000 PORTFOLIO_URL=http://127.0.0.1:8080 yarn build test --apply-lavamoat=false --build-type desktopextension",
    "test": "yarn lint && yarn test:unit && yarn test:unit:jest",
    "dapp": "node development/static-server.js node_modules/@metamask/test-dapp/dist --port 8080",
    "dapp-chain": "GANACHE_ARGS='-b 2' concurrently -k -n ganache,dapp -p '[{time}][{name}]' 'yarn ganache:start' 'sleep 5 && yarn dapp'",
    "forwarder": "node ./development/static-server.js ./node_modules/@metamask/forwarder/dist/ --port 9010",
    "dapp-forwarder": "concurrently -k -n forwarder,dapp -p '[{time}][{name}]' 'yarn forwarder' 'yarn dapp'",
    "test:unit": "./test/test-unit-combined.sh",
    "test:unit:jest": "./test/test-unit-jest.sh",
    "test:unit:global": "mocha test/unit-global/*.test.js",
    "test:unit:mocha": "mocha './app/**/*.test.js'",
    "test:e2e:chrome": "SELENIUM_BROWSER=chrome node test/e2e/run-all.js",
    "test:e2e:chrome:snaps": "yarn test:e2e:chrome --snaps",
    "test:e2e:chrome:desktop": "RUN_WITH_DESKTOP=true yarn test:e2e:chrome",
    "test:e2e:chrome:desktop:snaps": "yarn test:e2e:chrome:desktop --snaps",
    "test:e2e:firefox": "SELENIUM_BROWSER=firefox node test/e2e/run-all.js",
    "test:e2e:firefox:snaps": "yarn test:e2e:firefox --snaps",
    "test:e2e:firefox:desktop": "RUN_WITH_DESKTOP=true yarn test:e2e:firefox",
    "test:e2e:firefox:desktop:snaps": "yarn test:e2e:firefox:desktop --snaps",
