    "lavamoat:background:auto:ci": "node ./development/generate-lavamoat-policies.js --parallel=false",
    "lavamoat:auto": "yarn lavamoat:build:auto && yarn lavamoat:background:auto",
    "lavamoat:auto:ci": "yarn lavamoat:build:auto && yarn lavamoat:background:auto:ci",
    "lavamoat:desktop:ui:auto": "yarn lavamoat:desktop:ui:build:auto && yarn lavamoat:desktop:ui:background",
    "lavamoat:desktop:ui:background": "WRITE_AUTO_POLICY=1 yarn lavamoat:desktop:ui:build scripts:dist --policy-only",
    "lavamoat:desktop:ui:build:auto": "yarn lavamoat:desktop:ui:build scripts:dist --policy-only --writeAutoPolicy",
